<?xml version="1.0" encoding="utf-8"?>
<layer-list  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="0dp">
        <shape android:shape="rectangle">
            <gradient
                android:endColor="#055E5E"
                android:startColor="#2612BFBF"
                android:angle="90"
                android:type="linear" />
            <corners android:radius="20dp" />
        </shape>
    </item>
    <item  android:right="1dp" android:left="1dp" android:bottom="20dp">
        <shape android:shape="rectangle">
            <corners android:radius="20dp" />
            <stroke

                android:color="#06CCC0"
                android:width="2dp" />
        </shape>
    </item>
</layer-list>