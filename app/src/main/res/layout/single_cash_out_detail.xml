<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        android:orientation="vertical"
        android:layout_gravity="center_vertical|center_horizontal"
        android:layout_margin="20dp"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <TextView
            style="@style/huge_tittle_text_black"
            android:text="Detail Pengguna"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <LinearLayout
            android:layout_marginTop="12dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:textSize="14dp"
                android:layout_gravity="center_vertical"
                android:text="ID Pengguna"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"/>

            <TextView
                android:textSize="14dp"
                android:id="@+id/trxCashOutUserIs"
                android:layout_gravity="center_vertical"
                android:text="Nama Pengguna"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"/>

        </LinearLayout>

        <LinearLayout
                android:layout_marginTop="12dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <TextView
                    android:textSize="14dp"
                    android:layout_gravity="center_vertical"
                    android:text="Nama Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:textSize="14dp"
                    android:id="@+id/trxCashOutUserName"
                    android:layout_gravity="center_vertical"
                    android:text="Nama Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

        </LinearLayout>

        <LinearLayout
                android:layout_marginTop="12dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <TextView
                    android:textSize="14dp"
                    android:layout_gravity="center_vertical"
                    android:text="Email Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:textSize="14dp"
                    android:id="@+id/trxCashOutUserEmail"
                    android:layout_gravity="center_vertical"
                    android:text="Nama Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

        </LinearLayout>

    </LinearLayout>

    <Space android:layout_width="match_parent" android:layout_height="30dp"/>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <TextView
                style="@style/huge_tittle_text_black"
                android:text="Detail Transaksi"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        <LinearLayout
                android:layout_marginTop="12dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <TextView
                    android:textSize="14dp"
                    android:layout_gravity="center_vertical"
                    android:text="ID Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:textSize="14dp"
                    android:id="@+id/trxCashOutUserIs"
                    android:layout_gravity="center_vertical"
                    android:text="Nama Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

        </LinearLayout>

        <LinearLayout
                android:layout_marginTop="12dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <TextView
                    android:textSize="14dp"
                    android:layout_gravity="center_vertical"
                    android:text="Ref Number"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:textSize="14dp"
                    android:id="@+id/trxCashOutRefNumber"
                    android:layout_gravity="center_vertical"
                    android:text="Nama Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

        </LinearLayout>

        <LinearLayout
                android:layout_marginTop="12dp"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <TextView
                    android:textSize="14dp"
                    android:layout_gravity="center_vertical"
                    android:text="Email Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

            <TextView
                    android:textSize="14dp"
                    android:id="@+id/trxCashOutUserEmail"
                    android:layout_gravity="center_vertical"
                    android:text="Nama Pengguna"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"/>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>